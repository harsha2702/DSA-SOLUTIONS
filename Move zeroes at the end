// Problem: Given an array, move all zeros to the end without changing the order of non-zero elements.

#include <stdio.h>

void moveZeros(int arr[], int size) {
    int non_zero_index = 0;

    for (int i = 0; i < size; i++) {
        if (arr[i] != 0) {
            arr[non_zero_index++] = arr[i];
        }
    }
    for (int i = non_zero_index; i < size; i++) {
        arr[i] = 0;
    }
}

int main() {
    int arr[] = {0, 1, 0, 3, 12};
    int size = sizeof(arr) / sizeof(arr[0]);
    
    moveZeros(arr, size);
    
    for (int i = 0; i < size; i++) {
        printf("%d ", arr[i]);
    }  // Output: 1 3 12 0 0
    printf("\n");

    return 0;
}
