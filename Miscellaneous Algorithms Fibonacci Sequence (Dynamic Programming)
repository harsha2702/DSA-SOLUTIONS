// Problem: Find the nth Fibonacci number using dynamic programming.

#include <stdio.h>

#define MAX 100

long long fibonacci(int n) {
    long long dp[MAX] = {0};
    dp[1] = 1;
    
    for (int i = 2; i <= n; i++) {
        dp[i] = dp[i - 1] + dp[i - 2];
    }

    return dp[n];
}

int main() {
    int n = 50;
    printf("The %dth Fibonacci number is: %lld\n", n, fibonacci(n));  // Output: The 50th Fibonacci number is: 12586269025
    return 0;
}
