// Problem: Find the GCD (Greatest Common Divisor) of two numbers using Euclidean Algorithm

#include <stdio.h>

int gcd(int a, int b) {
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int main() {
    int a = 56, b = 98;
    
    printf("GCD of %d and %d is: %d\n", a, b, gcd(a, b));  // Output: 14

    return 0;
}
